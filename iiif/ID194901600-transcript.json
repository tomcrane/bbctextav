{
    "id": "https://tomcrane.github.io/bbctextav/iiif/ID194901600-transcript.json",
    "items": [
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sehr geehrter Herr Pr\u00e4sident!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-1",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=1.0,10.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sehr geehrte Kollegen!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-2",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=10.28,14.5",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Liebe Zuseher auf den Trib\u00fcnen und an den Fernsehbildschirmen!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-3",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=14.5,17.98",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Kommen wir zum Sport."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-4",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=17.98,19.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Der Anteil des Sporthaushalts am Gesamthaushalt ist von 1992 bis 2017 um 20\u00a0Prozent gesunken,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-5",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=19.44,28.22",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "auf unter 0,05\u00a0Prozent."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-6",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=28.22,32.3",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das sind j\u00e4mmerliche 49\u00a0Cent,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-7",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=32.3,34.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "auf 1\u00a0000 Euro gerechnet."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-8",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=34.28,37.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die absolute Summe lag im Jahr 2017 bei 170\u00a0Millionen Euro."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-9",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=37.04,42.54",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Zum Vergleich:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-10",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=42.54,43.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "F\u00fcr Sprachkurse f\u00fcr kulturfremde Migranten ist 1\u00a0Milliarde Euro vorhanden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-11",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=43.08,48.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "1\u00a0Milliarde!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-12",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=48.32,50.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das kleine Bundesland Hamburg hat 575\u00a0Millionen Euro f\u00fcr die Elbphilharmonie ausgegeben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-13",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=50.2,57.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "nicht wahr,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-14",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=57.48,58.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Scholz?\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-15",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=58.8,59.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ach,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-16",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=59.64,59.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "er ist gar nicht da.\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-17",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=59.08,61.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "F\u00fcr die eigenen Spitzensportler war aber nur ein Bruchteil dieser Summe \u00fcbrig."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-18",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=61.28,66.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ein deutscher Spitzensportler,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-19",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=66.92,67.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "falls er nicht beim Bund,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-20",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=67.28,68.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Zoll oder Polizei angestellt ist,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-21",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=68.16,71.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "hat ein Nettoeinkommen von unter 650\u00a0Euro,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-22",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=71.64,74.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ohne jede soziale Absicherung."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-23",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=74.6,76.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das ist nicht einmal der Mindestlohn."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-24",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=76.92,78.22",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Was haben Sie als Sportpolitiker eigentlich 25\u00a0Jahre lang im Sport\u00adausschuss getan?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-25",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=78.22,84.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie haben sich im besagten Zeitraum,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-26",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=84.76,90.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "von 1992 bis 2017,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-27",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=90.52,93.47999999999999",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die Di\u00e4ten um nominal sage und schreibe 133\u00a0Prozent erh\u00f6ht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-28",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=93.47999999999999,98.02000000000001",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dem Sporthaushalt aber nur 50\u00a0Prozent Erh\u00f6hung zugestanden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-29",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=98.02000000000001,103.06",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "\u00c4hnlich dem kategorischen Imperativ von Immanuel Kant gibt es den Spruch:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-30",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=103.06,108.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Was du nicht willst,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-31",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=108.08,109.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das man dir tut,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-32",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=109.2,110.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das f\u00fcg auch niemand anderem zu."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-33",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=110.16,112.24000000000001",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das hei\u00dft bei Ihnen:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-34",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=112.24000000000001,115.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Zuerst komme ich,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-35",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=115.32,117.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und wenn dann noch etwas \u00fcbrig ist,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-36",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=117.72,118.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "geben wir es den Sportlern."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-37",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=118.44,121.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wissen Sie was?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-38",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=121.28,123.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie sollten sich was sch\u00e4men!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-39",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=123.72,126.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Was sind die Folgen dieser K\u00fcrzung,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-40",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=126.36,128.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "in Kaufkraft gerechnet?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-41",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=128.2,130.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "50\u00a0Prozent aller Kaderathleten erw\u00e4gen das vorzeitige Karriereende."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-42",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=130.68,134.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Kanuten sollen pl\u00f6tzlich ihre Dopingkontrollen selber bezahlen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-43",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=134.24,138.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir haben einen Investitionsstau in Milliardenh\u00f6he bei Sportst\u00e4tten."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-44",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=138.44,142.14",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wegen besserer Bezahlung und langfristigerer Vertr\u00e4ge gehen Trainer ins Ausland."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-45",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=142.14,147.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir sind ja so ein reiches Land!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-46",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=147.8,149.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir als AfD-Fraktion haben viele Olympiast\u00fctzpunkte besucht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-47",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=149.4,153.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "als Beispiele Bad Kreuznach,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-48",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=153.88,154.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Leipzig und Halle."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-49",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=154.24,156.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Danach m\u00fcssen wir leider feststellen:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-50",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=156.6,158.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es liegt langj\u00e4hriges Politikversagen vor,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-51",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=158.76,160.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "welches dringend korrigiert werden muss."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-52",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194901600/canvas#t=160.04,162.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die AfD fordert daher die Verdopplung des Sportetats auf 340\u00a0Millionen Euro bis zum Jahr\u00a02020."
            },
