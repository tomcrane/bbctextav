{
    "id": "https://tomcrane.github.io/bbctextav/iiif/ID191700800-transcript.json",
    "items": [
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sehr geehrter Herr Pr\u00e4sident!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-1",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=1.0,3.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Liebe Kolleginnen und Kollegen!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-2",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=3.48,10.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Liebe Frauen!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-3",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=10.56,11.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Der Internationale Weltfrauentag am 8.\u00a0M\u00e4rz f\u00e4llt dieses Mal in ein ganz besonderes Jahr:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-4",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=11.28,20.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "100\u00a0Jahre Frauenwahlrecht in Deutschland."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-5",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=20.08,23.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Schon 100\u00a0Jahre?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-6",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=23.32,25.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Oder erst 100\u00a0Jahre?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-7",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=25.2,28.34",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Heute ist das Frauenwahlrecht f\u00fcr uns v\u00f6llig selbstverst\u00e4ndlich,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-8",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=28.34,32.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "so wie vieles andere eben auch."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-9",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=32.08,34.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Eigentlich k\u00f6nnen M\u00e4dchen und Frauen in Deutschland alles werden,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-10",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=34.28,40.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "alles machen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-11",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=40.52,41.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "jeden Beruf ergreifen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-12",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=41.72,42.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "den sie wollen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-13",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=42.04,43.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Vieles ist so selbstverst\u00e4ndlich,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-14",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=43.0,45.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass ich immer mal h\u00f6re:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-15",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=45.92,47.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Weltfrauentag in Deutschland?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-16",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=47.72,49.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Kampf f\u00fcr Frauenrechte und Gleichberechtigung\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-17",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=49.88,51.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wof\u00fcr brauchen wir das eigentlich noch?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-18",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=51.2,53.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "In unserem Grundgesetz steht:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-19",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=53.48,55.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "M\u00e4nner und Frauen sind gleichberechtigt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-20",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=55.2,60.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ja,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-21",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=60.84,60.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das steht von Beginn an in unserem Grundgesetz."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-22",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=60.36,64.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Und obwohl es seit 1949 drinsteht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-23",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=64.44,69.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "mussten noch 1977 Frauen ihre M\u00e4nner um Erlaubnis fragen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-24",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=69.76,74.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wenn sie arbeiten wollten."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-25",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=74.16,76.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das alles ist noch \u00fcberhaupt nicht lange her."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-26",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=76.0,79.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das Erreichte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-27",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=79.72,80.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "also das,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-28",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=80.6,80.03999999999999",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "was wir heute an Gleichberechtigung haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-29",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=80.03999999999999,83.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ist nicht einfach so vom Himmel gefallen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-30",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=83.72,86.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Nein,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-31",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=86.56,86.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "daf\u00fcr haben viele starke Frauen jahrelang gek\u00e4mpft."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-32",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=86.08,91.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Unsere M\u00fctter,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-33",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=91.8,92.03999999999999",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Omas,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-34",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=92.03999999999999,93.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Uromas haben sich daf\u00fcr eingesetzt und sich auch oft unbeliebt gemacht."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-35",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=93.6,98.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Davon profitieren wir bis heute."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-36",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=98.88,102.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "M\u00e4nner und Frauen sind gleichberechtigt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-37",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=102.72,109.24000000000001",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Artikel\u00a03 Absatz\u00a02 unseres Grundgesetzes ist aber keine Tatsache,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-38",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=109.24000000000001,114.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sondern ein st\u00e4ndiger Auftrag an uns alle,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-39",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=114.12,118.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die Augen aufzumachen und genau hinzusehen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-40",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=118.28,121.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wenn wir erkennen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-41",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=121.28,122.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Frauen irgendwo nicht gleichberechtigt sind und benachteiligt und diskriminiert werden,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-42",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=122.2,128.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dann m\u00fcssen wir handeln."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-43",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=128.24,131.86",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Uns und mir geht es dabei aber ausdr\u00fccklich nicht um ideologische Gleichmacherei."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-44",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=131.86,137.06",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "M\u00e4nner und Frauen m\u00fcssen nicht alles gleich machen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-45",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=137.06,141.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "M\u00e4nner und Frauen m\u00fcssen keine identischen Lebensbiografien haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-46",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=141.8,145.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und es m\u00fcssen sich auch nicht alle Paare die Erwerbst\u00e4tigkeit,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-47",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=145.04,149.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die Hausarbeit und die Familienarbeit gleichberechtigt teilen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-48",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=149.32,154.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir alle\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-49",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=154.56,154.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wenn ich von \u201ewir\u201c spreche,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-50",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=154.32,156.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dann meine ich die Frauen heute ebenso wie die M\u00e4nner\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-51",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=156.8,163.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "m\u00fcssen gar nichts,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-52",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID191700800/canvas#t=163.44,165.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "aber wir d\u00fcrfen,"
            },
