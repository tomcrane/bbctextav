            "id": "https://tomcrane.github.io/bbctextav/iiif/text-102",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194607600/canvas#t=267.44,268.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass wir im Haushalt\u00a02019 starke Korrekturen vornehmen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-103",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194607600/canvas#t=268.4,272.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Vielen Dank."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-104",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194607600/canvas#t=272.8,276.72",
            "type": "Annotation"
        },
