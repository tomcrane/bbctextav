            "id": "https://tomcrane.github.io/bbctextav/iiif/text-61",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19202200/canvas#t=207.96,209.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "damit der Terror besiegt werden kann\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-62",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19202200/canvas#t=209.16,212.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "auch f\u00fcr die Sicherheit unseres Landes."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-63",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19202200/canvas#t=212.56,213.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir leisten Beitr\u00e4ge f\u00fcr Stabspersonal,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-64",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19202200/canvas#t=213.12,221.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "f\u00fcr Marine\u00adeinheiten und f\u00fcr Luftaufkl\u00e4rung\u00a0\u2013"
