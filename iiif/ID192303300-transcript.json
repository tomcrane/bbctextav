        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "kann ich dem nur entgegnen:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-54",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192303300/canvas#t=160.2,161.38",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Frau Merkel war da ehrlicher."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-55",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192303300/canvas#t=161.38,165.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie hat von einer marktkonformen Demokratie geredet."
