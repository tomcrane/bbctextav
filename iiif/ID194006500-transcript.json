{
    "id": "https://tomcrane.github.io/bbctextav/iiif/ID194006500-transcript.json",
    "items": [
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Pr\u00e4sident!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-1",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=1.0,6.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Kolleginnen und Kollegen!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-2",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=6.64,13.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Liebe B\u00fcrgerinnen und B\u00fcrger!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-3",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=13.32,17.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wenn wir es nicht schaffen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-4",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=17.12,18.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die Klimaerhitzung auf deutlich unter 2\u00a0Grad zu begrenzen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-5",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=18.4,22.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "werden die Folgen\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-6",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=22.04,23.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ja\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-7",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=23.36,24.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dramatisch und unumkehrbar sein."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-8",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=24.88,27.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir bekommen eine Zunahme von Extremwetter;"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-9",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=27.72,30.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "da sind die Unwetter der letzten Wochen nur das Vorspiel."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-10",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=30.72,34.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Deshalb m\u00fcssen die Bundesregierung und die Autoindustrie endlich umsteuern."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-11",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=34.72,39.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das gesamte bisherige Gesch\u00e4ftsmodell der Autoindustrie\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-12",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=39.4,44.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "unbelehrbares Festhalten am Verbrennungsmotor,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-13",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=44.16,47.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "immer mehr Fahrzeuge,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-14",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=47.2,48.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "f\u00fcr die in den St\u00e4dten schon gar kein Platz mehr ist,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-15",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=48.28,51.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und immer gr\u00f6\u00dfere und schwerere Fahrzeuge mit immensem Spritverbrauch\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-16",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=51.0,57.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ist durch die fortschreitende Klimaerhitzung v\u00f6llig infrage gestellt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-17",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=57.56,62.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Der Verkehr ist der einzige Bereich,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-18",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=62.4,65.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "in dem der Aussto\u00df von Klimagasen immer weiter ansteigt;"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-19",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=65.64,68.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wir haben es heute schon geh\u00f6rt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-20",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=68.6,70.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die heutige Produktion von Autos\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-21",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=70.72,72.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Stichwort SUVs\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-22",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=72.72,73.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und die Verkehrsmittel des Welthandels\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-23",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=73.0,75.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Lkws und Schiffe\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-24",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=75.32,77.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sind alles andere als nachhaltig."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-25",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=77.92,80.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Um zumindest die Autos weniger klimasch\u00e4dlich zu machen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-26",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=80.8,83.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "m\u00fcssen sie leichter,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-27",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=83.12,84.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "verbrauchs\u00e4rmer und\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-28",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=84.48,87.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ja,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-29",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=87.8,87.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "auch das\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-30",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=87.44,88.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "weniger werden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-31",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=88.08,91.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Diesen Impuls muss die Politik endlich setzen und die notwendige Transformation der Autoindustrie ansto\u00dfen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-32",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=91.72,97.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "solange die Autoindustrie selber die offensichtlichen Entwicklungen weiter ignoriert."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-33",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=97.16,102.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die europ\u00e4ischen CO 2 -Grenzwerte sind das zentrale Instrument,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-34",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=102.2,107.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "um den Verbrauch der Fahrzeuge zu verringern."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-35",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=107.92,109.24000000000001",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Dabei ist von entscheidender Bedeutung,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-36",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=109.24000000000001,111.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass die Vorgaben den tats\u00e4chlichen Bedarf abdecken,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-37",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=111.36,114.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "also eigentlich noch erh\u00f6ht werden m\u00fcssen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-38",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=114.36,117.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Dass neue Autos verbrauchsarm sind,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-39",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=117.32,119.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ist selbst f\u00fcr CO 2 -arme Antriebe wichtig."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-40",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=119.44,123.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Je leichter und verbrauchs\u00ad\u00e4rmer das Fahrzeug,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-41",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=123.88,126.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "desto leichter ist der \u00dcbergang zu CO 2 -freien Antrieben."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-42",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=126.72,129.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Bei der Elektromobilit\u00e4t\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-43",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=129.24,131.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wir haben es eben schon ansatzweise geh\u00f6rt\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-44",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=131.88,133.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "kann dann beispielsweise die Batterie viel kleiner ausfallen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-45",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=133.0,136.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das hei\u00dft,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-46",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=136.44,137.92000000000002",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "auch der gr\u00f6\u00dfte Kostenfaktor bei einer Umstellung wird dadurch preisg\u00fcnstiger."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-47",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=137.92000000000002,141.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Aber was n\u00fctzen die besten Grenzwerte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-48",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=141.84,144.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wenn sie durch Betr\u00fcgereien und Manipulationen auf den Pr\u00fcfst\u00e4nden umgangen werden?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-49",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=144.0,149.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Auch bei den CO 2 -Werten klafft ja ein ganzer Abgrund zwischen Messung und Wirklichkeit:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-50",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=149.84,154.07999999999998",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ganze 40\u00a0Prozent Abweichung."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-51",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=154.07999999999998,157.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "F\u00fcr die Verbraucherinnen und Verbraucher\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-52",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=157.8,158.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das haben wir eben schon vom Kollegen K\u00fchn geh\u00f6rt\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-53",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=158.32,162.92000000000002",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sind aber verl\u00e4ssliche Angaben zum Schadstoffaussto\u00df wichtig."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-54",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=162.92000000000002,165.07999999999998",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie sind es,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-55",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=165.07999999999998,166.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die die Zeche f\u00fcr die Manipulationen der Autokonzerne aktuell zahlen m\u00fcssen;"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-56",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=166.88,170.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wir erleben das gerade."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-57",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=170.96,171.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Eine Verminderung des Aussto\u00dfes von Klimagasen macht sich auch in deren Portemonnaie bemerkbar."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-58",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=171.44,177.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Starke Klimaziele sind also auch im Interesse der Verbraucher."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-59",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=177.0,180.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Dazu braucht es aber mehr Transparenz."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-60",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=180.24,184.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die CO 2 -Zahlen aus den Messungen unter realen Fahrbedingungen m\u00fcssen ver\u00f6ffentlicht werden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-61",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=184.04,190.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Nur so k\u00f6nnen sich die Autok\u00e4ufer orientieren und richtig informiert Entscheidungen treffen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-62",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=190.96,196.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "\u00dcbrigens sind gerade Menschen mit geringem Einkommen auf billigen Wohnraum angewiesen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-63",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=196.6,203.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Und wo finden sie diesen Wohnraum in den St\u00e4dten heute noch?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-64",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=203.48,207.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "An lauten und luftverpesteten Stra\u00dfen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-65",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=207.16,210.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Aber auch diese Menschen haben einen Anspruch darauf,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-66",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=210.24,213.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "kosteng\u00fcnstig mobil sein zu k\u00f6nnen und kosteng\u00fcnstig wohnen zu k\u00f6nnen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-67",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=213.0,217.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und das unter gesunden Bedingungen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-68",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=217.44,220.54",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Linke fordert daher unter anderem einen breiten Ausbau des \u00d6PNV zum Nulltarif und einen starken Ausbau f\u00fcr den Rad-"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-69",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=220.54,228.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und Fu\u00dfverkehr sowie gute L\u00f6sungen f\u00fcr die Reduzierung des motorisierten Individualverkehrs in den St\u00e4dten."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-70",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=228.24,235.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Bundesregierung muss die St\u00e4dte und Gemeinden beim Umbau im Zuge der Verkehrswende zugunsten dieses Umweltverbundes unterst\u00fctzen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-71",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=235.84,243.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und sie muss endlich eine effektive Kontrolle der Autoindustrie gew\u00e4hrleisten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-72",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=243.12,247.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "damit die europ\u00e4ischen Grenzwerte eingehalten werden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-73",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=247.48,251.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "So sichert man Arbeitspl\u00e4tze!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-74",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=251.88,253.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Vielen Dank f\u00fcr Ihre Aufmerksamkeit."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-75",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=253.48,255.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herzlichen Dank,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-76",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=255.44,256.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Frau Kollegin Remmers.\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-77",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=256.16,257.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Als N\u00e4chstes der fraktionslose Abgeordnete Mario Mieruch."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-78",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194006500/canvas#t=257.36,261.0",
