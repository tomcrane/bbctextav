{
    "id": "https://tomcrane.github.io/bbctextav/iiif/ID19704800-transcript.json",
    "items": [
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sehr geehrte Frau Pr\u00e4sidentin!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-1",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=1.0,9.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Meine sehr verehrten Damen und Herren!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-2",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=9.28,12.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "F\u00e4llt Ihnen eigentlich auf,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-3",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=12.64,14.7",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass trotz Boom die Armut nicht abnimmt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-4",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=14.7,19.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "aber der Reichtum f\u00fcr wenige exorbitant steigt?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-5",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=19.12,22.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ist das f\u00fcr Sie ein Problem,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-6",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=22.4,24.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "oder akzeptieren Sie das einfach so?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-7",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=24.48,27.7",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich glaube,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-8",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=27.7,28.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wir m\u00fcssen dar\u00fcber nachdenken,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-9",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=28.92,30.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass die Verteilung in unserem Land tats\u00e4chlich\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-10",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=30.52,34.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das sagen nicht nur wir,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-11",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=34.68,36.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sondern auch alle Institute\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-12",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=36.12,37.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ein Ausma\u00df angenommen hat,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-13",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=37.24,39.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das nicht mehr in Ordnung ist,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-14",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=39.84,40.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "weil es neben den ganzen moralischen Aspekten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-15",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=40.2,43.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die damit verbunden sind,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-16",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=43.8,44.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die wirtschaftliche Entwicklung massiv behindert."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-17",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=44.8,47.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Woran liegt das?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-18",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=47.88,48.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es liegt zum einen daran,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-19",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=48.56,49.3",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass die Verteilung des Volkseinkommens,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-20",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=49.3,52.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "also das Verh\u00e4ltnis zwischen L\u00f6hnen und Profiten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-21",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=52.32,55.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "nicht mehr stimmt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-22",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=55.88,56.26",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Der Anteil der L\u00f6hne am Volkseinkommen lag im Jahr\u00a02000 bei 71,9\u00a0Prozent."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-23",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=56.26,65.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Inzwischen betr\u00e4gt er 68,5\u00a0Prozent."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-24",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=65.72,68.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das ist der Anteil der L\u00f6hne am gesamten erwirtschafteten Kuchen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-25",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=68.2,72.5",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "H\u00e4tten die Arbeitnehmer noch die Lohnquote des Jahres\u00a02000,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-26",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=72.5,76.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dann h\u00e4tten sie allein im Jahr\u00a02017\u00a0 82\u00a0Milliarden Euro mehr."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-27",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=76.4,84.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "H\u00e4tten wir die Verteilung des Volkseinkommens aus dem Jahr\u00a02000 kumuliert seit dem Jahr\u00a02000,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-28",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=84.16,93.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dann h\u00e4tten die Arbeitnehmerinnen und Arbeitnehmer,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-29",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=93.6,95.18",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "also die normalen Menschen hierzulande,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-30",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=95.18,97.52000000000001",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "1,4\u00a0Billionen Euro mehr in der Tasche."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-31",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=97.52000000000001,99.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die entscheidende Frage lautet:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-32",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=99.44,102.75999999999999",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wohin geht das Geld,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-33",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=102.75999999999999,103.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und wo liegen die Ursachen?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-34",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=103.2,105.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Ursachen f\u00fcr diese ungleiche Prim\u00e4rverteilung bzw."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-35",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=105.68,109.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "diese falsche Entwicklung sind Niedriglohn,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-36",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=109.72,111.03999999999999",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "befristete Besch\u00e4ftigung und Leiharbeit."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-37",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=111.03999999999999,114.82",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ulrike Herrmann hat es in der \u201etaz\u201c folgenderma\u00dfen formuliert\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-38",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=114.82,117.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ich zitiere\u00a0\u2013:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-39",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=117.6,118.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Agenda\u00a02010 hat nicht nur die Ungleichheit vor 2005 versch\u00e4rft\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-40",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=118.56,124.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dieses politische Lohndumping sorgt jetzt daf\u00fcr,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-41",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=124.56,128.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass sogar in der Hochkonjunktur die Ungleichheit nicht abnimmt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-42",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=128.28,133.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sondern tendenziell weiter steigt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-43",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=133.72,135.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Recht hat sie,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-44",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=135.0,136.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und das m\u00fcssen wir ver\u00e4ndern."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-45",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=136.04,138.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Eine M\u00f6glichkeit,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-46",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=138.88,144.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "der Ungleichheit entgegenzuwirken,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-47",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=144.56,145.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "bietet die Steuerpolitik,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-48",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=145.32,146.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die f\u00fcr eine andere Sekund\u00e4rverteilung sorgen kann."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-49",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=146.24,148.3",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "In diesem Zusammenhang bietet sich sicherlich die Anhebung des Spitzensteuersatzes an."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-50",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=148.3,151.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Meine Damen und Herren von der CSU,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-51",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=151.2,153.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "was halten Sie eigentlich von folgendem Satz:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-52",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=153.12,156.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "\u201eArbeitsloses Einkommen arbeitsf\u00e4higer Personen wird nach Ma\u00dfgabe der Gesetze mit Sondersteuern belegt.\u201c\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-53",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=156.88,162.22",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wo sind denn die Kollegen von der CSU?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-54",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=162.22,165.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "\u2013\u00a0Max,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-55",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=165.36,166.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "findest du das in Ordnung?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-56",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=166.88,168.06",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Du m\u00fcsstest das eigentlich in Ordnung finden;"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-57",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=168.06,172.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "denn dieser Satz steht in der bayerischen Verfassung."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-58",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=172.64,176.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wenn wir alle in Bayern lebten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-59",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=176.68,180.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "m\u00fcssten wir durch Steuern f\u00fcr einen gewissen Ausgleich sorgen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-60",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=180.72,184.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Nach Angaben der Bundesbank befindet sich der Gro\u00dfteil des Immobilienverm\u00f6gens in den H\u00e4nden der oberen 20\u00a0Prozent."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-61",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=184.28,193.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "In den sieben gr\u00f6\u00dften St\u00e4dten in Deutschland sind die Preise von Wohnimmobilien von 2007 bis 2016 um etwa 65\u00a0Prozent gestiegen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-62",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=193.6,200.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Mietpreise folgen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-63",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=200.44,202.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Hier findet eine krasse Umverteilung statt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-64",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=202.88,204.46",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "aber von unten nach oben."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-65",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=204.46,207.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir m\u00fcssen bei der Verm\u00f6gensverteilung etwas tun."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-66",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=207.28,211.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Der DGB stellt in seinem Verteilungsbericht fest\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-67",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=211.68,214.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ich zitiere\u00a0\u2013:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-68",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=214.68,214.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "\u201eIn keinem gr\u00f6\u00dferen Industriestaat der OECD werden Verm\u00f6gen so stark geschont wie in Deutschland.\u201c Tats\u00e4chlich kommen in Deutschland nur 2,6\u00a0Prozent des Gesamtsteueraufkommens aus verm\u00f6gensbezogenen Steuern."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-69",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=214.48,228.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "In den USA sind es 10,8\u00a0Prozent und in Frankreich 8,5\u00a0Prozent."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-70",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=228.64,234.92000000000002",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir h\u00e4tten hier also durchaus Spielraum."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-71",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=234.92000000000002,237.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Verteilung ist nicht nur ungerecht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-72",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=237.88,240.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sondern behindert auch das Wirtschaftswachstum."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-73",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=240.56,244.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Deshalb schlagen wir Ihnen die l\u00e4ngst \u00fcberf\u00e4llige Einf\u00fchrung der Verm\u00f6gensteuer vor."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-74",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=244.28,248.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Diese soll tats\u00e4chlich nur die Menschen betreffen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-75",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=248.28,251.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die ein Verm\u00f6gen von mehr als 1 Million Euro haben."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-76",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=251.76,255.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Besteuert werden soll nur das oberhalb des Freibetrags von 1\u00a0Million Euro liegende Nettoverm\u00f6gen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-77",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=255.68,261.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und zwar mit einem Steuersatz von 5\u00a0Prozent."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-78",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=261.4,264.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wenn Sie das machen w\u00fcrden,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-79",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=264.92,265.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "h\u00e4tten wir tats\u00e4chlich die M\u00f6glichkeit,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-80",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=265.16,268.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wieder eine einigerma\u00dfen vern\u00fcnftige Verm\u00f6gensverteilung hinzubekommen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-81",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=268.6,272.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie k\u00f6nnten dann wieder ruhig schlafen\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-82",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=272.32,274.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "auch der Max\u00a0\u2013,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-83",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=274.72,275.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "weil wir uns entsprechend der bayerischen Verfassung verhielten."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-84",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=275.56,277.15999999999997",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich danke f\u00fcrs Zuh\u00f6ren."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-85",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=277.15999999999997,285.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "N\u00e4chster Redner ist der Abgeordnete Christian Freiherr von Stetten f\u00fcr die CDU/CSU-Fraktion."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-86",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19704800/canvas#t=285.56,291.0",
            "type": "Annotation"
        }
    ],
    "type": "AnnotationPage"
