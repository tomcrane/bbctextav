        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "haben offensichtlich selbst Sie erkannt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-54",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193402600/canvas#t=144.32,147.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie haben jetzt eine Pflegekraft pro Pflegeeinrichtung angek\u00fcndigt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-55",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193402600/canvas#t=147.96,151.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das reicht aber bei weitem nicht aus."
