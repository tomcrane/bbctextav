{
    "id": "https://tomcrane.github.io/bbctextav/iiif/ID194905100-transcript.json",
    "items": [
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Pr\u00e4sident!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-1",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=1.0,11.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Meine Damen und Herren!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-2",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=11.76,13.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Diese Debatte zum Wirtschaftsetat erinnert mich ein bisschen an die Debatte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-3",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=13.2,19.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die wir vor zwei Tagen mit dem Finanzminister hatten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-4",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=19.24,24.26",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und ich meine das in kritischer Weise,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-5",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=24.26,26.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "um das gleich klarzustellen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-6",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=26.44,29.66",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es ist so,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-7",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=29.66,29.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass ich wahrnehme,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-8",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=29.36,30.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass die Gro\u00dfe Koalition sich sehr daran gew\u00f6hnt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-9",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=30.28,34.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass die Lage so gut ist."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-10",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=34.28,36.06",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Im \u00f6ffentlichen Haushalt ist die Lage ganz gut,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-11",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=36.06,40.66",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und auch die wirtschaftlichen Daten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-12",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=40.66,42.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "was Besch\u00e4ftigung,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-13",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=42.4,44.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wachstum und die St\u00e4rke der deutschen Wirtschaft angeht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-14",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=44.6,47.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sind aktuell nicht zu beklagen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-15",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=47.44,50.78",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Aber\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-16",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=50.78,52.5",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das fehlt mir hier in der Debatte\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-17",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=52.5,54.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "man kann doch nicht \u00fcbersehen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-18",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=54.44,57.42",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass die Herausforderungen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-19",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=57.42,60.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die ganz nah anstehen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-20",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=60.76,61.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "im n\u00e4chsten Jahrzehnt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-21",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=61.36,63.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und die Ver\u00e4nderungen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-22",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=63.16,65.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die sich durch Digitalisierung,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-23",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=65.6,67.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Globalisierung und die Notwendigkeit der \u00f6kologischen Transformation ergeben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-24",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=67.68,72.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "gro\u00dfe Herausforderungen sind und die Voraussetzungen daf\u00fcr darstellen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-25",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=72.12,77.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass wir zuk\u00fcnftig im Weltmarkt erfolgreich sein k\u00f6nnen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-26",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=77.4,82.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das ist die Aufgabe der Politik."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-27",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=82.32,85.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich habe eben nicht geh\u00f6rt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-28",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=85.52,86.03999999999999",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Sie diese Herausforderungen adressieren,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-29",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=86.03999999999999,88.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sondern Sie benutzen die gegenw\u00e4rtigen Daten dazu,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-30",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=88.08,92.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sich zur\u00fcckzulehnen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-31",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=92.8,93.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Mattfeldt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-32",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=93.8,94.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das ist das,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-33",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=94.72,94.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "was uns umtreibt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-34",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=94.32,96.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und das kann man ganz konkret beschreiben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-35",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=96.16,98.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Minister."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-36",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=98.28,99.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Wettbewerbsf\u00e4higkeit der deutschen Wirtschaft muss sich zuk\u00fcnftig durch Innovation und,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-37",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=99.4,108.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ich w\u00fcrde sagen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-38",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=108.64,109.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die zwingende Verschr\u00e4nkung der digitalen Revolution mit einer Ressourceneffizienzrevolution ergeben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-39",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=109.88,117.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und das wird kein Selbstg\u00e4nger sein."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-40",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=117.08,122.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir haben kreative Menschen in unserer Gesellschaft und in der Wirtschaft,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-41",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=122.08,127.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "aber die verdienen auch gute politische Leitplanken,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-42",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=127.64,130.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "damit das gelingt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-43",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=130.96,131.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Zu den politischen Rahmenbedingungen geh\u00f6rt nat\u00fcrlich,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-44",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=131.28,137.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "diese g\u00fcnstige Konjunkturlage zu nutzen und die \u00dcbersch\u00fcsse,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-45",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=137.04,140.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die wir haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-46",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=140.6,141.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "f\u00fcr strukturelle Reformen einzusetzen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-47",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=141.6,144.34",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Hier wurde von Herrn Linnemann von positiven Standortfaktoren gesprochen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-48",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=144.34,151.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich sage:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-49",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=151.96,152.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ja,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-50",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=152.6,152.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sicherheit ist ein positiver Standortfaktor."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-51",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=152.48,157.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Aber positive Standortfaktoren sind auch eine offene Gesellschaft und das Bekenntnis zur Zuwanderung von Fachkr\u00e4ften."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-52",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=157.52,167.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "In dieser Frage fehlt mir die Einmischung des Wirtschaftsministers"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-53",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=167.72,172.92000000000002",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "in die v\u00f6llig erratische Debatte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-54",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=172.92000000000002,174.07999999999998",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die in der Union gef\u00fchrt wird."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-55",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=174.07999999999998,176.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es ist doch kein Zufall,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-56",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=176.16,178.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Kanzlerin Merkel gestern in ihrer Rede fast ein bisschen nachdenklich gesagt hat:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-57",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=178.68,183.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die R\u00fcckmeldung aus der Wirtschaft ist,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-58",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=183.44,186.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass man sich im Moment gar nicht so viele Sorgen um die steuerpolitischen Rahmenbedingungen der Unternehmen macht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-59",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=186.76,191.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sondern mehr Sorgen um andere Rahmenbedingungen:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-60",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=191.16,193.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es geht darum,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-61",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=193.72,193.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "\u00fcberhaupt Fachkr\u00e4fte zu finden,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-62",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=193.2,195.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und es geht um den Standortfaktor \u201eWie entwickelt sich Deutschland im internationalen Image?\u201c."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-63",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=195.04,201.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Da geht es leider nach unten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-64",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=201.96,203.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und da hat auch der Wirtschaftsminister eine Aufgabe."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-65",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=203.28,207.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich komme noch zu einem weiteren Faktor:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-66",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=207.16,209.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ein wichtiger Standortfaktor ist zweifelsohne auch das Innovationsklima."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-67",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=209.84,216.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich finde es nach wie vor unverst\u00e4ndlich\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-68",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=216.56,218.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wir haben das schon h\u00e4ufiger angeprangert\u00a0\u2013:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-69",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=218.88,220.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ausgerechnet die Mittel f\u00fcr die industrielle Gemeinschaftsforschung,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-70",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=220.28,223.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die sehr wirksam ist f\u00fcr kleine und mittlere Unternehmen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-71",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=223.24,227.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Minister,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-72",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=227.8,228.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "werden im Vergleich zum Vorjahr gek\u00fcrzt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-73",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=228.68,232.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Gerade kleine und mittlere Unternehmen haben keine eigenen gro\u00dfen Forschungsabteilungen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-74",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=232.76,237.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sie m\u00fcssen sich zusammenschlie\u00dfen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-75",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=237.04,239.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sie brauchen diese Programme."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-76",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=239.76,241.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie wissen auch,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-77",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=241.6,242.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass es mehr gute Antr\u00e4ge gibt und deswegen die Ablehnungsquote hoch ist."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-78",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=242.88,246.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Deswegen gibt es \u00fcberhaupt keinen Grund,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-79",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=246.04,249.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass diese Programme in Ihrem Haus gek\u00fcrzt werden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-80",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=249.56,252.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Da erwarten wir von den Haushaltsberatungen Nachbesserungen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-81",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=252.84,255.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Letzter Punkt zum Abschluss."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-82",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=255.88,257.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Insgesamt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-83",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=257.44,258.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Minister,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-84",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=258.28,259.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "erwarte ich von Ihnen einen ganz anderen Impuls und auch eine ganz andere Managementqualit\u00e4t,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-85",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=259.36,267.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "endlich daf\u00fcr zu sorgen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-86",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=267.64,269.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass die \u00f6kologische Transformation der Wirtschaft sich auch in der Arbeit Ihres Hauses niederschl\u00e4gt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-87",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=269.72,276.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es gibt bislang kein einziges Programm,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-88",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=276.96,278.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das sich mit dieser Transformation gezielt besch\u00e4ftigt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-89",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=278.32,282.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie k\u00f6nnen das doch nicht ernsthaft der Umweltministerin alleine \u00fcberlassen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-90",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=282.64,285.15999999999997",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "G\u00a07 in Elmau hat damals als gro\u00dfes Ziel die Dekarbonisierung der Industrie in diesem Jahrhundert ausgegeben."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-91",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=285.15999999999997,294.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Da hat der Wirtschaftsminister eine origin\u00e4re Aufgabe."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-92",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=294.56,296.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Daf\u00fcr m\u00fcssen Sie Gesetze vorlegen und Programme gestalten;"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-93",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=296.0,299.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das ist leider alles Fehlanzeige."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-94",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=299.96,301.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Reden und reisen reicht nicht\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-95",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=301.44,303.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sondern machen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-96",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=303.4,305.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sch\u00f6nen Dank."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-97",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=305.16,311.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Vielen Dank.\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-98",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=311.52,311.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Als N\u00e4chster redet Karl Holmeier f\u00fcr die Fraktion der CDU/CSU."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-99",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID194905100/canvas#t=311.2,315.0",
            "type": "Annotation"
        }
