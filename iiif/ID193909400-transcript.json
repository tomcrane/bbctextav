{
    "id": "https://tomcrane.github.io/bbctextav/iiif/ID193909400-transcript.json",
    "items": [
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Sichert,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-1",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=1.0,5.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ich hatte Ihnen schon zugetraut,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-2",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=5.0,8.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Sie zuh\u00f6ren,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-3",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=8.28,10.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Sie an einer Stelle zuh\u00f6ren,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-4",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=10.2,13.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wo Sie namentlich angesprochen werden,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-5",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=13.2,16.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und hier nicht wild die Unwahrheit sagen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-6",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=16.84,19.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Vielleicht haben Sie nicht richtig zugeh\u00f6rt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-7",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=19.12,21.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "oder Sie sagen bewusst die Unwahrheit."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-8",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=21.92,23.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich habe nicht behauptet,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-9",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=23.24,26.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Sie die Barrierefreiheitsrichtlinie als schwachsinnig bezeichnet h\u00e4tten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-10",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=26.04,30.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sondern ich habe gesagt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-11",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=30.36,31.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Sie in einer behindertenpolitischen Debatte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-12",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=31.4,34.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "in die Sie sich anscheinend verirrt haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-13",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=34.72,36.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "weil Sie zu einem vollkommen anderen Thema geredet haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-14",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=36.8,39.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "zweimal das Wort \u201eschwachsinnig\u201c verwendet haben."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-15",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=39.8,43.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es ist \u00fcberall in dieser Gesellschaft nicht in Ordnung,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-16",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=43.96,46.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ein absolutes No-Go,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-17",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=46.44,48.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "diesen Begriff zu verwenden,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-18",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=48.32,50.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "weil er absolut behindertenfeindlich ist"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-19",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=50.56,52.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und weil er zeigt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-20",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=52.08,53.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Sie nicht verstanden haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-21",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=53.32,55.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "worum es hier geht:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-22",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=55.04,56.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "um die gleichberechtigte Teilhabe von Menschen mit Behinderung in dieser Gesellschaft."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-23",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=56.24,62.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie haben eine riesige Gruppe von Menschen verletzt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-24",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=62.56,64.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "aber das scheint die Art zu sein,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-25",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=64.12,66.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wie die AfD in diesem Land Politik macht."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-26",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=66.08,77.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Und es kotzt mich an!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-27",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=77.6,78.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "N\u00e4chster Redner in der Debatte:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-28",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=78.72,80.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Dr.\u00a0Matthias Bartke f\u00fcr die SPD-Fraktion."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-29",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID193909400/canvas#t=80.64,84.0",
            "type": "Annotation"
        }
    ],
    "type": "AnnotationPage"
