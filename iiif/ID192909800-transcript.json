    "items": [
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Pr\u00e4sident!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-1",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=1.0,3.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Meine Damen und Herren!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-2",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=3.84,6.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Frau Dr\u00f6ge,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-3",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=6.6,7.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ich konnte Ihrem Antrag beim Lesen durchaus einiges abgewinnen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-4",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=7.68,11.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich habe bei Ihrer Rede leider das ein oder andere Fragezeichen hinzubekommen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-5",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=11.08,18.54",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich stehe nicht unbedingt in Verdacht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-6",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=18.54,20.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "den Bundeswirtschaftsminister zu verteidigen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-7",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=20.84,22.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "aber mich hat schon irritiert,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-8",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=22.96,24.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Sie hier infrage stellen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-9",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=24.68,25.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass man bei der Digitalisierung nicht \u201enur\u201c\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-10",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=25.04,28.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das war das Zitat,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-11",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=28.12,29.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das Sie richtig wiedergegeben haben\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-12",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=29.04,31.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "\u00fcber die Risiken sprechen darf."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-13",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=31.92,32.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das ist ein schmaler Grat."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-14",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=32.32,34.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "In Ihrer \u00dcberschrift betonen Sie das doch eigentlich,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-15",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=34.2,37.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass es n\u00e4mlich ebenso um einen fairen Wettbewerb geht\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-16",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=37.24,40.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wettbewerb als Grundlage dessen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-17",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=40.8,42.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wie unsere Wirtschaft aufgebaut ist\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-18",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=42.4,45.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wie um Datenschutz und das Recht jedes Einzelnen auf informationelle Selbstbestimmung."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-19",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=45.92,52.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Insofern wundert mich das schon ein bisschen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-20",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=52.64,55.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich habe heute Morgen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-21",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=55.12,58.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "nachdem ich die \u201eFAZ\u201c lesen konnte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-22",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=58.56,59.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "nachvollzogen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-23",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=59.12,61.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass es Ihnen vielleicht mehr darum ging,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-24",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=61.76,63.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "mit Ihrem Antrag die \u00dcberschriften in dem Punkt zu dominieren,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-25",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=63.92,67.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Facebook und WhatsApp getrennt werden m\u00fcssen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-26",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=67.6,70.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wenn das am Ende die Kernaussage Ihrer digitalpolitischen Agenda und dieses Antrags ist,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-27",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=70.76,76.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dann ist das doch ein bisschen armselig."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-28",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=76.88,78.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich gebe Ihnen an einer Stelle ausdr\u00fccklich recht:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-29",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=78.08,82.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Facebook hat\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-30",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=82.4,83.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das ist f\u00fcr mich \u00fcberhaupt nicht nachvollziehbar\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-31",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=83.4,85.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "in der Anh\u00f6rung die Marktmacht der eigenen Stellung verneint."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-32",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=85.36,90.66",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das ist aus unserer Sicht sehr,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-33",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=90.66,92.52000000000001",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sehr fragw\u00fcrdig,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-34",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=92.52000000000001,93.24000000000001",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "um nicht zu sagen:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-35",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=93.24000000000001,94.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "falsch."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-36",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=94.44,95.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich mache das an einem Beispiel fest,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-37",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=95.32,97.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das ab und zu bem\u00fcht wird."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-38",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=97.36,99.3",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Stellen wir uns ein Caf\u00e9 oder eine Kneipe vor."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-39",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=99.3,102.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wo gehen Sie hin?"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-40",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=102.36,104.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie gehen nat\u00fcrlich dahin,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-41",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=104.56,105.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wo Ihre Freunde sind."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-42",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=105.36,107.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie gehen ja nicht in die leere Kneipe eine Ecke weiter,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-43",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=107.72,109.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "nur weil Sie irgendwie aus Prinzip die Konkurrenz unterst\u00fctzen wollen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-44",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=109.2,113.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das alleine macht deutlich,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-45",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=113.32,116.66",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "welche faktische Macht dieses Unternehmen hat."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-46",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=116.66,119.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Hinzu kommt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-47",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=119.32,120.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass die Kneipe \u201eFacebook\u201c 24\u00a0Stunden ge\u00f6ffnet hat,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-48",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=120.2,124.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und das f\u00fcr t\u00e4glich 23\u00a0Millionen Nutzer aus Deutschland."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-49",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=124.72,128.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir brauchen also mehr Auswahl;"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-50",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=128.88,129.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das ist richtig."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-51",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=129.44,131.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie haben die Dateninteroperabilit\u00e4t\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-52",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID192909800/canvas#t=131.64,134.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
