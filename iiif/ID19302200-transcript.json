{
    "id": "https://tomcrane.github.io/bbctextav/iiif/ID19302200-transcript.json",
    "items": [
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Pr\u00e4sident!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-1",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=1.0,4.74",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Meine Damen und Herren!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-2",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=4.74,8.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Schia gegen Sunni,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-3",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=8.56,9.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Stellvertreterkrieg zwischen Iran und Saudi-Arabien\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-4",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=9.92,11.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "es gibt so viele einfache Erkl\u00e4rungsmuster,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-5",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=11.48,15.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die das,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-6",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=15.24,16.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "was im Nahen Osten zurzeit geschieht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-7",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=16.56,17.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "beschreiben sollen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-8",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=17.32,19.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Diese Muster sind nicht alle falsch,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-9",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=19.8,21.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "aber sie sind auch nicht immer ganz richtig."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-10",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=21.76,23.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es wird bei diesen Pauschalurteilen immer wieder verkannt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-11",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=23.68,27.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass es auch lokale Ursachen gibt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-12",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=27.52,28.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die dann von ausw\u00e4rtigen Faktoren noch verst\u00e4rkt werden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-13",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=28.12,32.3",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Der Anlass f\u00fcr den Beginn der Auseinandersetzungen in Syrien war,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-14",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=32.3,36.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass auf Eltern geschossen wurde,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-15",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=36.6,38.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die friedlich dagegen demonstriert haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-16",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=38.92,40.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass ihre Kinder mit Folterspuren nach Hause gekommen sind."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-17",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=40.08,44.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Der Konflikt im Jemen hat eine lange Geschichte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-18",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=44.2,48.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die zur\u00fcckgeht auf Auseinandersetzungen zwischen den Huthi in der Peripherie und dem Zentrum,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-19",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=48.04,52.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "bei denen es auch um Ressourcen und um Wasser geht."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-20",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=52.68,55.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Im Jemen ist dieser Konflikt aber massiv verst\u00e4rkt worden von einer Auseinandersetzung,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-21",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=57.84,63.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die zu einer unglaublichen humanit\u00e4ren Katastrophe gef\u00fchrt hat."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-22",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=63.88,67.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Zweieinhalb Jahre l\u00e4uft nun dieser Krieg,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-23",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=67.12,70.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "der den Jemen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-24",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=70.88,71.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das traditionell \u00e4rmste Land der Arabischen Liga,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-25",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=71.44,75.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "in die Steinzeit gebombt hat."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-26",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=75.44,77.1",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Nun haben wir seit \u00fcber zwei Wochen eine Totalblockade des Nordens des Landes,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-27",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=77.1,84.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-28",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=84.56,84.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wie es aussieht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-29",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=84.92,85.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "zum Ziel hat,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-30",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=85.12,87.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ganze Landteile regelrecht auszuhungern."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-31",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=87.56,90.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "940\u00a0000\u00a0F\u00e4lle von Cholera gibt es aktuell;"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-32",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=90.52,94.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die H\u00e4lfte davon betrifft Kinder."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-33",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=94.16,96.52000000000001",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Zweieinhalb Millionen Menschen haben keinen Zugang zu sauberem Wasser."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-34",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=96.52000000000001,99.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "T\u00e4glich sterben 130\u00a0Kinder an den Folgen des Krieges und der Blockade;"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-35",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=99.32,105.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die H\u00e4lfte aller Kinder im gesamten Land ist mittlerweile durch Hunger in ihrer Entwicklung gest\u00f6rt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-36",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=105.72,112.47999999999999",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Saudi-Arabien und die Allianz um Saudi-Arabien haben diesen Krieg nicht begonnen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-37",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=112.47999999999999,117.58",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und sie haben ihn auch nicht verschuldet."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-38",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=117.58,118.47999999999999",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ausl\u00f6ser war die illegitime Macht\u00ad\u00fcbernahme durch die Huthi."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-39",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=118.47999999999999,122.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Aber die Art und Weise der Kriegsf\u00fchrung f\u00fchrt zu einer humanit\u00e4ren Katastrophe ohnegleichen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-40",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=122.04,128.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-41",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=128.28,128.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ehrlich gesagt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-42",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=128.48,129.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ich empfinde es als unglaublich sch\u00e4ndlich,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-43",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=129.0,132.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass dieser Konflikt nicht die Aufmerksamkeit bekommt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-44",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=132.0,135.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die ihm aufgrund der katastrophalen Lage zukommt\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-45",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=135.68,136.92000000000002",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und das nur,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-46",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=136.92000000000002,137.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "weil aus geografischen Gr\u00fcnden aus dem Land keine Menschen fliehen und als Fl\u00fcchtlinge zu uns kommen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-47",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=137.04,145.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ja,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-48",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=145.68,145.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "der Iran facht den Konflikt weiter an durch Unterst\u00fctzung f\u00fcr die Huthi,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-49",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=145.16,149.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "durch Unterst\u00fctzung f\u00fcr den Ex-Pr\u00e4sidenten Saleh,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-50",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=149.84,151.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "durch eine verantwortungslose Rhetorik."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-51",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=151.4,154.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir d\u00fcrfen aber nicht verkennen\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-52",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=154.0,156.62",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das entgegne ich Ihnen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-53",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=156.62,157.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Kollege Dr.\u00a0Gauland,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-54",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=157.72,159.07999999999998",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "auf Ihre Aussage,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-55",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=159.07999999999998,160.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wir h\u00e4tten da sowieso keinen Einfluss\u00a0\u2013,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-56",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=160.6,161.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Deutschland an diesem Krieg auch nicht ganz unbeteiligt ist durch die Waffen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-57",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=161.16,165.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die wir dorthin geliefert haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-58",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=165.84,167.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die auch eingesetzt worden sind und auch weiterhin eingesetzt werden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-59",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=167.64,171.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich habe mir,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-60",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=171.84,172.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Kollege Lambsdorff,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-61",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=172.36,174.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "tats\u00e4chlich mehrfach angeschaut,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-62",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=174.88,175.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "was Au\u00dfenminister Gabriel so gesagt hat."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-63",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=175.8,177.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich finde nicht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-64",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=177.36,179.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass er einen falschen Ton angeschlagen hat."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-65",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=179.56,181.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Er hat in erster Linie Sorgen vor einer massiven Eskalation ge\u00e4u\u00dfert,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-66",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=181.96,184.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die aus meiner Sicht v\u00f6llig berechtigt waren."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-67",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=184.48,188.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Diese Rhetorik,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-68",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=188.96,189.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die richtig ist,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-69",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=189.72,190.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "muss aber durch einen Stopp der Waffenlieferungen unterf\u00fcttert werden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-70",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=190.72,202.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es ist keine Frage,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-71",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=202.04,202.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass wir mit dem Iran und mit Saudi-Arabien sprechen m\u00fcssen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-72",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=202.48,205.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Beide Staaten sind aber als strategische Partner nicht geeignet."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-73",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=205.48,210.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir brauchen \u00c4quidistanz,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-74",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=210.12,212.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wobei die Betonung auf Distanz liegt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-75",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=212.88,214.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Rolle des Irans im Nahen Osten ist auch weiterhin hochaggressiv."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-76",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=214.36,217.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "In Syrien sieht man das dramatisch."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-77",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=217.44,220.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Er unterst\u00fctzt gemeinsam mit Russland einen Pr\u00e4sidenten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-78",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=220.6,223.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "der Giftgas einsetzt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-79",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=223.76,224.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "der Fassbomben wirft und der auch ganze Landstriche aushungern l\u00e4sst."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-80",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=224.96,228.92000000000002",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Oder nehmen wir den Libanon:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-81",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=228.92000000000002,229.22",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Unterst\u00fctzung der Hisbollah im Libanon stellt nicht nur eine Bedrohung f\u00fcr den Staat Israel dar,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-82",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=229.22,236.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "sondern bringt auch eine massive Schw\u00e4chung der innenpolitischen Lage und der Institutionen im Libanon mit sich\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-83",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=236.2,242.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "auch das muss sehr klar angesprochen werden\u00a0\u2013,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-84",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=242.4,245.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und zwar zu einer Zeit,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-85",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=245.48,247.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "zu der der Libanon mehr als 1\u00a0Million Fl\u00fcchtlinge aufgenommen hat."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-86",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=247.44,251.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Genau deswegen kommt ja gerade niemand auf die Idee,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-87",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=251.76,253.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "an den Iran Waffen zu verkaufen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-88",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=253.48,256.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Genau aus denselben Gr\u00fcnden sollte auch niemand auf die Idee kommen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-89",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=256.92,259.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Saudi-Arabien Waffen zu verkaufen und auch nicht den Staaten,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-90",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=259.36,263.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die an dem Konflikt beteiligt sind und die die humanit\u00e4re Katastrophe im Jemen angezettelt haben."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-91",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=263.12,268.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir m\u00fcssen Distanz zu den Staaten bewahren,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-92",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=268.36,272.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die die Feuer in Syrien,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-93",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=272.0,274.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "im Libanon,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-94",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=274.64,275.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "im Irak,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-95",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=275.64,275.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "im Jemen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-96",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=275.48,277.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "in Bahrain und in vielen anderen Staaten der Region weiter entfachen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-97",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=277.52,280.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Am Ende des Tages wird es auch darum gehen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-98",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=280.64,281.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ein waches Auge zu haben f\u00fcr die lokalen Ursachen der Konflikte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-99",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=281.32,285.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die es dort gibt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-100",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=285.0,286.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herzlichen Dank.\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-101",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=286.8,286.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Als N\u00e4chster:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-102",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=286.4,287.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Alexander Radwan f\u00fcr die CDU/CSU-Fraktion."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-103",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19302200/canvas#t=287.28,294.0",
