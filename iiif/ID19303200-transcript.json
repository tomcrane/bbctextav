{
    "id": "https://tomcrane.github.io/bbctextav/iiif/ID19303200-transcript.json",
    "items": [
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Pr\u00e4sident!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-1",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=1.0,6.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Verehrte Kolleginnen!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-2",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=6.08,7.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Verehrte Kollegen!"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-3",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=7.92,9.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich will versuchen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-4",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=9.56,9.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "das,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-5",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=9.08,10.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "was wir soeben geh\u00f6rt haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-6",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=10.8,12.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "in den gro\u00dfen historischen Kontext unseres Landes zu stellen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-7",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=12.52,15.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und in diesem Zusammenhang darauf hinweisen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-8",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=15.96,18.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Deutschland schon immer ein Einwanderungsland gewesen ist."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-9",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=18.64,21.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wenn wir in der Geschichte zur\u00fcckgehen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-10",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=21.2,25.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "stellen wir fest,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-11",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=25.92,26.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Einwanderer dieses Land schon immer mit gepr\u00e4gt haben,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-12",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=26.8,28.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "seien es die Hugenotten im 17.\u00a0Jahrhundert,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-13",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=28.36,31.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Menschen aus Polen im 19.\u00a0Jahrhundert,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-14",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=31.96,33.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Menschen aus S\u00fcdeuropa im 20.\u00a0Jahrhundert,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-15",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=33.16,37.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die von dem Wirtschaftswunder dieses Landes angelockt wurden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-16",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=37.84,40.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Auch ohne hier geboren zu sein,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-17",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=40.96,42.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "haben viele Menschen dieses Land mit gepr\u00e4gt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-18",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=42.16,45.92",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "mit entwickelt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-19",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=45.92,46.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "f\u00fcr Wohlstand und Stabilit\u00e4t in diesem Land gesorgt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-20",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=46.04,55.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Deswegen muss man gar keine Angst vor Einwanderung haben."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-21",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=55.4,62.78",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Unsere Gesellschaft schrumpft und altert ohne Zuwanderung."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-22",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=62.78,65.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Angesichts unseres Fachkr\u00e4ftebedarfs ist es sinnvoll,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-23",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=65.2,69.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "unser Land f\u00fcr Zuwanderer und Einwanderer zu \u00f6ffnen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-24",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=69.48,74.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wenn sie einen Beitrag zu Wohlstand und Stabilit\u00e4t leisten k\u00f6nnen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-25",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=74.68,79.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Nat\u00fcrlich muss es daf\u00fcr Regeln geben."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-26",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=79.84,83.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Die Regeln,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-27",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=83.8,84.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die im Gesetzentwurf der SPD zu finden sind,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-28",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=84.2,87.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Kollege Hartmann,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-29",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=87.8,88.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "gehen aus unserer Sicht durchaus in die richtige Richtung."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-30",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=88.08,91.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Erlauben Sie mir als Rechtspolitiker den Hinweis,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-31",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=91.4,93.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass das Urheberrecht dieser Gedanken bei der FDP anzusiedeln ist."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-32",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=93.36,97.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Peter Caesar hat bereits in den 90er-Jahren als Justizminister in Rheinland-Pfalz einen Entwurf vorgelegt,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-33",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=97.16,104.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "der in diese Richtung geht."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-34",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=104.16,105.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie haben ihn weiterentwickelt."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-35",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=105.08,107.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Aber ich will darauf hinweisen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-36",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=107.28,110.96000000000001",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass von unserer Seite schon immer eine Offenheit f\u00fcr diese Themen bestanden hat."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-37",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=110.96000000000001,115.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Kollege,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-38",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=115.0,116.64",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "erlauben Sie mir eine kurze Unterbrechung Ihrer Rede,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-39",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=116.64,118.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "weil meiner Bitte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-40",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=118.12,120.66",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "im Plenarsaal nicht zu fotografieren,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-41",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=120.66,122.76",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ausgerechnet von zwei Mitgliedern meiner Fraktion nicht entsprochen wurde."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-42",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=122.76,124.44",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich weise noch einmal darauf hin,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-43",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=124.44,128.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass das Fotografieren im Plenarsaal nach Beschluss des Pr\u00e4sidiums untersagt ist."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-44",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=128.52,132.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Der N\u00e4chste,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-45",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=132.96,133.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "den ich erwische,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-46",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=133.52,135.62",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "bekommt einen Ordnungsruf."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-47",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=135.62,139.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Lassen Sie mich einen Blick auf Ihren Gesetzentwurf werfen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-48",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=139.84,143.56",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "in dem Sie ein Punktesystem entwickeln."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-49",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=143.56,145.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Grundlage sind verschiedene Kriterien wie Berufsqualifikation,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-50",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=145.32,149.16",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sprachkenntnisse,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-51",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=149.16,150.28",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Alter,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-52",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=150.28,151.24",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Integrationsaspekte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-53",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=151.24,152.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Berufserfahrung und Vorliegen eines Arbeitsplatzangebotes."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-54",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=152.48,156.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das sind gute Ans\u00e4tze."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-55",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=156.8,157.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ich will nur beim Thema Integrations\u00adaspekte auf Folgendes hinweisen:"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-56",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=157.36,161.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Es ist sicherlich richtig,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-57",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=161.04,162.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass Menschen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-58",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=162.32,163.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die unter anderen Aspekten ins Land gekommen sind\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-59",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=163.12,165.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "zum Beispiel weil sie Schutz vor Verfolgung oder vor Krieg und B\u00fcrgerkrieg gesucht haben\u00a0\u2013,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-60",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=165.4,170.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "einen Integrationsvorsprung vorweisen k\u00f6nnen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-61",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=170.96,172.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das soll aber nicht zum Nachteil solcher Menschen gereichen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-62",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=172.12,175.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die sich von ihrer Heimat aus bewerben und einen solchen Integrationsvorsprung nicht besitzen k\u00f6nnen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-63",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=175.36,181.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Hier gibt es ein paar Punkte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-64",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=181.32,183.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "\u00fcber die wir gerne noch einmal diskutieren m\u00f6chten."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-65",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=183.72,186.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Dazu z\u00e4hlt auch die Kontingentierung."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-66",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=186.68,188.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sie schlagen vor,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-67",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=188.8,189.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass der Bundestag auf Vorschlag der Bundesregierung und mit Zustimmung des Bundesrates j\u00e4hrlich die Zahl der Menschen festlegen soll,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-68",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=189.88,197.68",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die in unser Land einwandern k\u00f6nnen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-69",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=197.68,199.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das erscheint uns etwas planwirtschaftlich."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-70",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=199.0,203.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das Problem in der Vergangenheit bestand ja nicht darin,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-71",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=203.88,207.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass zu viele hochqualifizierte Zuwanderer zu uns gekommen sind."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-72",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=207.8,211.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Der Gesetzentwurf geht in die richtige Richtung."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-73",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=211.04,215.2",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Er findet bei uns Zuspruch und Sympathie."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-74",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=215.2,218.92000000000002",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Er springt aber in ein paar Punkten vielleicht etwas zu kurz."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-75",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=218.92000000000002,221.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Sinn eines Punktesystems ist,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-76",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=221.4,223.12",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "eine Zuwanderung von Fachkr\u00e4ften auch ohne Arbeitsvertrag zu erm\u00f6glichen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-77",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=223.12,228.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Ansonsten brauchten wir gar kein Punktesystem."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-78",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=228.48,232.8",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Aus unserer Sicht reicht es auch nicht,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-79",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=232.8,234.72",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass es \u201eeinen legalen Einwanderungsweg f\u00fcr Drittstaatsangeh\u00f6rige unterhalb der Voraussetzungen der Blauen Karte EU\u201c,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-80",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=234.72,240.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "wie es im Gesetzentwurf hei\u00dft,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-81",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=240.36,243.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "geben soll."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-82",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=243.52,243.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Wir meinen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-83",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=243.0,244.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass die Regelungen betreffend die Blaue Karte EU grundlegend reformiert und in einem Einwanderungsgesetz zusammengef\u00fchrt werden m\u00fcssen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-84",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=244.6,251.84",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Dabei sollten Asylbewerber und Asylberechtigte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-85",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=251.84,254.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Fl\u00fcchtlinge aus B\u00fcrgerkriegs-"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-86",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=254.88,255.36",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "und Kriegsgebieten sowie Fachkr\u00e4fte,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-87",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=255.36,258.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "die zu uns kommen wollen,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-88",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=258.96,259.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "gleicherma\u00dfen ber\u00fccksichtigt werden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-89",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=259.4,262.0",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Aus unserer Sicht w\u00e4re es sinnvoll,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-90",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=262.0,263.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "eine Reform der Blue-Card-Regelungen f\u00fcr Einwanderer mit Arbeitsplatz\u00adangebot durchzuf\u00fchren und parallel dazu ein Punktesystem f\u00fcr Einwanderer ohne Arbeitsplatzangebot einzuf\u00fchren."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-91",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=263.48,279.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Das entspr\u00e4che unseren Vorstellungen."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-92",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=279.6,279.4",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Aber ohne Zweifel ist es Ihr Vorschlag wert,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-93",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=279.4,282.96",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "im Hauptausschuss behandelt zu werden."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-94",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=282.96,284.6",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Deswegen stimmen wir f\u00fcr die \u00dcberweisung und eine eingehende Beratung."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-95",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=284.6,288.88",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Vielen Dank."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-96",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=288.88,288.48",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herzlichen Dank,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-97",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=288.48,291.08",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Herr Kollege Thomae.\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-98",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=291.08,293.15999999999997",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "Als N\u00e4chstes\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-99",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=293.15999999999997,294.04",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "ich hoffe,"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-100",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=294.04,295.52",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "dass ich den Namen richtig ausspreche\u00a0\u2013"
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-101",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=295.52,297.32",
            "type": "Annotation"
        },
        {
            "body": {
                "language": "de",
                "type": "TextualBody",
                "value": "hat Zaklin Nastic das Wort f\u00fcr die Fraktion Die Linke."
            },
            "id": "https://tomcrane.github.io/bbctextav/iiif/text-102",
            "motivation": "supplementing",
            "target": "https://tomcrane.github.io/bbctextav/iiif/ID19303200/canvas#t=297.32,301.0",
            "type": "Annotation"
        }
    ],
